package org.akinu.events
{
	import com.adobe.cairngorm.control.CairngormEvent;
	
	import flash.events.Event;

	public class StateChangeEvent extends CairngormEvent
	{
		public static const STATE_CHANGE = "StateChage";
		public var state:uint;
		public function StateChangeEvent(state:uint){
			super(STATE_CHANGE);
			this.state = state;
		}
		override function clone():Event{
			return new StateChangeEvent(this.state);
		}
		
	}
}