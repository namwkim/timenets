package org.akinu.events
{
	import com.adobe.cairngorm.control.CairngormEvent;
	
	import flash.events.Event;
	
	import genvis.GenVis;
	import genvis.data.Person;

	public class InitializeEvent extends CairngormEvent
	{
		public static const INITIALIZE:String = "Initialize";
		public var root:Person;
		public var vis:GenVis;
		public function InitializeEvent(root:Person, vis:GenVis) {
			super(INITIALIZE);
			this.root = root;
			this.vis  = vis;
			
		}
		override public function clone():Event{
			return new InitializeEvent(root, vis);
		}
	}
}